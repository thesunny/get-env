{
  "name": "@thesunny/get-env",
  "version": "1.0.1",
  "main": ".npm/src/index.js",
  "license": "MIT",
  "types": ".npm/src/index.d.ts",
  "files": [
    ".npm/**/*"
  ],
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.15",
    "jest": "^27.4.7",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.4.0",
    "tsc": "^2.0.4",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.5.5"
  },
  "scripts": {
    "-- fix": "# fix syntax",
    "fix:prettier": "yarn lint:prettier --write",
    "-- lint": "# lint syntax",
    "lint:prettier": "prettier --check \"**/*.{css,md,js,jsx,json,ts,tsx}\"",
    "-- preset": "# Run preset from @thesunny/presets",
    "preset": "ts-node --project node_modules/@thesunny/presets/tsconfig.ts-node.json node_modules/@thesunny/presets/bin/index.ts",
    "--- test": "#",
    "test:once": "jest --config=jest.config.js",
    "test:watch": "jest --watch --config=jest.fast.config.js",
    "test:watch:types": "jest --watch --config=jest.config.js",
    "test:clearcache": "jest --clearCache",
    "--- npm": "# npm package scripts",
    "build:npm:once": "rm -rf ./.npm/ && yarn test:once && tsc -p tsconfig.ts-build.json && echo 'Finished Building'",
    "build:npm:watch": "rm -rf ./.npm/ && tsc -p tsconfig.ts-build.json --watch",
    "publish:npm:patch": "yarn test:once && yarn build:npm:once && yarn version --patch && echo '\"npm publish --access=public\" to publish to npm'"
  },
  "dependencies": {
    "@thesunny/assert-type": "^0.1.7"
  }
}
